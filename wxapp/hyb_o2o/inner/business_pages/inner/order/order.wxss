@import "../../../../../__wuBaseWxss__/5.wxss";

page {
    background: #F7F7F7;
}

.top {
    background: #FAFAFA;
    padding: 20rpx 0;
    color: #313131;
    border-bottom: 2rpx solid #D9D9D9;
    position: relative;
}

.back {
    color: #242424;
    position: absolute;
    left: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.right_arr {
    transform: rotateY(180deg);
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.title {
    background: #fff;
    padding: 0 30rpx;
    justify-content: space-around;
}

.title_item {
    padding: 20rpx 40rpx;
    border-bottom: 2rpx solid #fff;
}

.title .sel {
    color: #02CCBA;
    border-bottom: 2rpx solid #02CCBA;
}

.list_item {
    padding: 20rpx;
    background: #fff;
    padding-bottom: 0;
}

.bot {
    text-align: right;
}