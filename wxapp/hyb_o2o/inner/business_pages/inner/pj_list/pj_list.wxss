@import "../../../../../__wuBaseWxss__/5.wxss";

page {
    background: #F7F7F7;
}

.title {
    background: #FAFAFA;
    padding: 0 30rpx;
    justify-content: space-around;
    border-bottom: 2rpx solid #D9D9D9;
}

.title_item {
    padding: 20rpx 40rpx;
    border-bottom: 2rpx solid #fff;
}

.title .sel {
    color: #02CCBA;
    border-bottom: 2rpx solid #02CCBA;
}

.swith_btn {
    transform: scale(0.6);
    float: right;
}

.item {
    width: 100%;
    padding: 20rpx;
    background: #fff;
    box-sizing: border-box;
}

.text {
    width: 80%;
}

.item .img {
    width: 150rpx;
    height: 150rpx;
    margin-right: 20rpx;
}

.money {
    color: #FE5639;
}