@import "../../../__wuBaseWxss__/5.wxss";

page {
    background: #f5f5f5;
    padding-top: 20rpx;
}

.part {
    padding: 0 20rpx;
    background: #fff;
}

.icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;
}

.pick {
    width: 100rpx;
}

.input {
    width: 100%;
    text-align: center;
}

.sanjiao {
    width: 30rpx;
    height: 30rpx;
    transform: rotate(90deg);
}

.choose_box {
    border: 2rpx solid #ABABAB;
    border-radius: 10rpx;
    margin-left: 20rpx;
}

.canvas {
    width: 100%;
    height: 500rpx;
    background: #fff;
}

.kuan {
    background: #FBF9F7;
    color: #333;
    width: 49.5%;
    padding: 20rpx 0;
    text-align: center;
    margin-bottom: 5rpx;
}