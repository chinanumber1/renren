@import "../../../__wuBaseWxss__/5.wxss";

@import "../../template/template.wxss";

page {
    background: #f3f3f3;
}

.choose_list {
    background: #fff;
    color: #333;
    margin-bottom: 20rpx;
}

.choose_list .pick {
    width: 33.33%;
    text-align: center;
    justify-content: center;
}

.choose_list .pick input {
}

.choose_list picker {
    width: 64%;
}

.sanjiao {
    width: 14rpx;
    height: 20rpx;
    transform: rotate(90deg);
    margin-left: 20rpx;
}

.s_ok {
    color: #E52C2C;
}

.s_ing {
    color: #8FE9C7;
}

.item {
    margin-bottom: 20rpx;
    padding: 0 20rpx;
    background: #fff;
    font-size: 28rpx;
}

.item .mid .m_r {
    width: 80%;
    position: relative;
}

.item .img {
    width: 150rpx;
    height: 150rpx;
    margin-right: 20rpx;
}

.item .business_icon {
    width: 35rpx;
    height: 35rpx;
    margin-right: 10rpx;
}

.item .budget {
    position: absolute;
    top: 0rpx;
    right: 20rpx;
}

.item .qiang {
    color: #fff;
    background: #EF4A50;
    padding: 10rpx 20rpx;
    border-radius: 5rpx;
}

.no_more {
    width: 100%;
    text-align: center;
    padding-top: 100rpx;
}

.no_more image {
    width: 200rpx;
    height: 200rpx;
    margin-bottom: 20rpx;
}