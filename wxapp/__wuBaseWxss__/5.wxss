.nomore {
    line-height: 60rpx;
    height: 60rpx;
    text-align: center;
    position: relative;
    font-size: 24rpx;
    width: 100%;
    color: #999;
}

.nomore::before {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #999;
    position: absolute;
    left: 180rpx;
    top: 50%;
}

.nomore::after {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #999;
    position: absolute;
    right: 180rpx;
    top: 50%;
}

